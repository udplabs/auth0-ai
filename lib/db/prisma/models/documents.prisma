model Document {
  id          String   @id @default(ulid())
  pageContent String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  embedding   Json
  metadata    Json

  source Transaction @relation(fields: [id], references: [id], onDelete: Cascade)
}

model SampleDocument {
  id          String   @id @default(ulid())
  pageContent String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  embedding   Json
  metadata    Json
}
