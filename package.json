{
	"name": "auth0-ai-devcamp",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"debug": "next dev",
		"build": "pnpm db:generate && next build",
		"start": "next start",
		"lint": "next lint",
		"db:generate": "pnpm db:generate:prisma && pnpm db:generate:neon",
		"db:generate:prisma": "prisma generate --schema ./lib/db/prisma",
		"db:generate:neon": "prisma generate --schema ./lib/db/neon",
		"db:migrate": "pnpm db:migrate:prisma && pnpm db:migrate:neon",
		"db:migrate:prisma": "prisma migrate dev --schema ./lib/db/prisma",
		"db:migrate:neon": "prisma migrate dev --schema ./lib/db/neon",
		"db:studio": "prisma studio --schema ./lib/db/prisma --port 5555",
		"db:schema:validate": "prisma validate"
	},
