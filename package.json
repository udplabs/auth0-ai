{
	"name": "auth0-ai-devcamp",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbo",
		"debug": "next dev",
		"build": "prisma generate && next build",
		"start": "next start",
		"lint": "next lint",
		"db:generate": "pnpm db:generate:prisma && pnpm db:generate:neon",
		"db:generate:prisma": "prisma generate --schema ./lib/db/prisma --no-engine",
		"db:generate:neon": "prisma generate --schema ./lib/db/neon --no-engine",
		"db:migrate": "pnpm db:migrate:prisma && pnpm db:migrate:neon",
		"db:migrate:prisma": "prisma migrate dev --schema ./lib/db/prisma",
		"db:migrate:neon": "prisma migrate dev --schema ./lib/db/neon",
		"db:studio": "pnpm db:studio:prisma && pnpm db:studio:neon",
		"db:studio:prisma": "prisma studio --schema ./lib/db/prisma --port 5555",
		"db:studio:neon": "prisma studio --schema ./lib/db/neon --port 5556",
		"db:schema:validate": "prisma validate",
		"test": "export PLAYWRIGHT=True && pnpm exec playwright test"
	},
