{
	"name": "auth0-ai-devcamp",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"lab-init": "npm run db:generate && npm run dev",
		"debug": "next dev",
		"build": "npm run db:generate && next build",
		"start": "next start",
		"lint": "next lint",
		"analyze": "ANALYZE=true npm build",
		"type-check": "tsc --noEmit",
		"db:generate": "npm run db:generate:prisma && npm run db:generate:supabase",
		"db:generate:prisma": "prisma generate --schema ./lib/db/prisma",
		"db:generate:supabase": "prisma generate --schema ./lib/db/supabase",
		"db:migrate": "npm run db:migrate:prisma && npm run db:migrate:supabase",
		"db:migrate:prisma": "prisma migrate dev --schema ./lib/db/prisma",
		"db:migrate:supabase": "prisma migrate dev --schema ./lib/db/supabase",
		"db:studio": "prisma studio --schema ./lib/db/prisma --port 5555",
		"db:schema:validate": "prisma validate"
	},
